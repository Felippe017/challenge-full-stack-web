generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Students {
  id           Int     @default(autoincrement())
  name         String? @db.VarChar(255)
  email        String  @db.VarChar(255)
  registration Int     @unique
}
